function[SNR] = ERLE(M, desired, error)
N = size(desired,2);
%d = meansqr(desired(1,M:N));
d = desired.^2;
e = error.^2;
SNR = zeros(N-M+1);
for i=M:N
    SNR(i) = 10*log(d(i)/e(i-M+1));
end;
%plot(1:size(SNR,2),SNR(1,:));
%xlabel('number of iteration');
%ylabel('Sound to Noise ratio');
title('Improvement in sound to noise ratio');
set (gca, 'fontsize', 18);
%ylim(00);
end